{
	"author": {
		"email": "rblackman24@gmail.com"
	},
	"background": {
		"service_worker": "sw.js",
		"type": "module"
	},
	"chrome_url_overrides": {
		"newtab": "index.html"
	},
	"description": "Personalizable new tab page for Tottenham fans. Keep track of the league results and fixtures. Add links to your most visited sites.",
	"host_permissions": ["https://tottenham-api.vercel.app/api/fixtures"],
	"icons": {
		"32": "public/logo-32.png",
		"64": "public/logo-64.png",
		"128": "public/logo-128.png",
		"512": "public/logo-512.png"
	},
	"key": "",
	"manifest_version": 3,
	"minimum_chrome_version": "118",
	"name": "Spurs New Tab Page",
	"options_page": "options.html",
	"permissions": ["alarms", "storage", "tabs"],
	"short_name": "COYS Tab",
	"version": "0.2.13"
}
